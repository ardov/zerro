{
  "name": "zerro",
  "version": "0.10.6",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "deploy": "craco build && cd ./build && now --prod",
    "eject": "react-scripts eject",
    "lint:css": "stylelint './src/**/*.js'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "stylelint": {
    "defaultSeverity": "warning",
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-sass-guidelines",
      "stylelint-config-styled-components"
    ],
    "plugins": [
      "stylelint-order",
      "stylelint-config-rational-order/plugin"
    ],
    "rules": {
      "order/properties-order": [],
      "order/properties-alphabetical-order": null,
      "plugin/rational-order": true,
      "declaration-block-trailing-semicolon": null
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/react": "^2.1.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.1",
    "@mui/styles": "^5.8.0",
    "@mui/system": "^5.8.1",
    "@react-hook/window-scroll": "^1.3.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/browser": "^6.13.2",
    "@types/mdx-js__react": "^1.5.5",
    "browserslist": "^4.18.1",
    "comlink": "^4.3.0",
    "convert-layout": "^0.11.1",
    "cookies-js": "^1.2.3",
    "date-fns": "^2.21.1",
    "effector": "^22.3.0",
    "formik": "^2.2.9",
    "history": "^4.10.1",
    "idb": "^6.0.0",
    "json2mq": "^0.2.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "qrcode.react": "^1.0.0",
    "react": "^18.1.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^18.1.0",
    "react-ga": "^3.2.0",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^3.4.0",
    "react-redux": "^8.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.1",
    "recharts": "^2.1.6",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.7.2",
    "use-local-storage-state": "^12.0.0",
    "uuid": "^8.3.2",
